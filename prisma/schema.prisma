datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model Game {
  id        String   @id @default(cuid())
  takenAt DateTime @default(now())
  score     Int      @default(0)
  time      Int      @default(0)
  accuracy  Int      @default(0)
  userId    String

  @@index([userId])
}
